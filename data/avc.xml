      <parent name="30fps" />
    </restriction>

    <restriction type="video" used="in-strict">
      <parent name="AVC_MP" />
      <parent name="9/16_VGA_4:3" />
      <parent name="30fps" />
    </restriction>

    <restriction type="video" used="in-strict">
      <parent name="AVC_MP" />
      <parent name="9/16_VGA_16:9" />
      <parent name="30fps" />
    </restriction>

    <restriction type="video" used="in-strict">
      <parent name="AVC_MP" />
      <parent name="625_1/2D1" />
