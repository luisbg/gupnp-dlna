    </restriction>

    <restriction type="video" used="in-strict">
      <parent name="AVC_MP" />
      <parent name="1/9_VGA_4:3" />
      <parent name="30fps" />
    </restriction>

    <restriction type="video" used="in-strict">
      <parent name="AVC_MP" />
